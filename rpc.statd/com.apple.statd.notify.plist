<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.apple.statd.notify</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/sbin/rpc.statd</string>
		<string>-n</string>
	</array>
	<key>LaunchOnlyOnce</key>
	<true/>
	<key>KeepAlive</key>
	<dict>
		<key>PathState</key>
		<dict>
			<key>/var/db/statd.status</key>
			<true/>
		</dict>
		<key>Crashed</key>
		<true/>
	</dict>
</dict>
</plist>
